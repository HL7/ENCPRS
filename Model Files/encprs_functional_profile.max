      <tag name="Reference.FunctionID" value="AS.2" />
      <tag name="Reference.CriterionID" value="1" />
      <tag name="Reference.ChangeIndicator" value="NC" />
      <tag name="Priority" value="EN" />
    </object>
    <object>
      <id>000358e3-b5f0-4848-a5ab-3a477db502a9</id>
      <name>AS.2#02</name>
      <alias />
      <notes>The system SHOULD provide the ability to transmit a notification to an external system (e.g., an external Client Registry or a Personal Health Record System) that a patient's demographic information was modified.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>2bfb3627-7058-420f-a37a-d2f5734ea5d3</parentId>
      <tag name="Row" value="1429" />
