      <tag name="Reference.Alias" value="EHRS_FM_R2.0.1 March 2017" />
      <tag name="Reference.FunctionID" value="AS.2" />
      <tag name="Reference.CriterionID" value="2" />
      <tag name="Reference.ChangeIndicator" value="NC" />
      <tag name="Priority" value="EN" />
    </object>
    <object>
      <id>cca049dc-34c6-4dd5-a0a6-95094fa18a78</id>
      <name>AS.2#03</name>
      <alias />
      <notes>The system SHOULD provide the ability to tag patient information with the current status (e.g., active, admitted, inactive, or discharged).</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>2bfb3627-7058-420f-a37a-d2f5734ea5d3</parentId>
      <tag name="Row" value="1430" />
      <tag name="Optionality" value="SHOULD" />
      <tag name="Conditional" value="N" />
